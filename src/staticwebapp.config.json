{
  "routes": [
    {
      "route": "/css/*.css",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/login/*.css",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "*.css",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/login",
      "allowedRoles": ["anonymous"],
      "rewrite": "/.auth/login/aadb2c"
    },
    {
      "route": "/logout",
      "allowedRoles": ["anonymous"],
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/index2.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/img/*", "/js/*", "*.css", "*.js", "*.json"]
  },
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "rewrite": "/login"
    }
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "aadb2c": {
          "registration": {
            "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://OilWrxB2cTenant.b2clogin.com/OilWrxB2cTenant.onmicrosoft.com/B2C_1_myswa_signup_signin2/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": [],
            "loginParameterNames": []
          }
        }
      }
    }
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css"
  }
}
